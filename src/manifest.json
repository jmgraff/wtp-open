{
    "name": "Website Tracker Pro",
    "version": "1.3",
    "description": "Track anything you want on the web, get email notifications when they change, and more!",
    "permissions": [
      "http://*/*",
      "https://*/*",
      "notifications",
      "identity",
      "identity.email",
      "tabs"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
    ],
    "web_accessible_resources": [
      "content.css"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "name": "Website Tracker Pro"
    },
    "web_accessible_resources": [
    ],
    "icons": {
      "128": "images/Logo.png"
    },
    "oauth2": {
      "client_id": "194265630443-13k1k832ons4912udn1cgrc3rvofmrut.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/gmail.send"
      ]
    },
    "manifest_version": 2
}
